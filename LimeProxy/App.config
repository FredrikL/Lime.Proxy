<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="listen" value="http://localhost:8000"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IDataService" />
        <binding name="BasicHttpBinding_IMetaDataService" />
        <binding name="BasicHttpBinding_ISessionService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:23183/" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IDataService" contract="LimeWs.IDataService"
        name="BasicHttpBinding_IDataService" />
      <endpoint address="http://localhost:23183/" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IMetaDataService" contract="LimeWs.IMetaDataService"
        name="BasicHttpBinding_IMetaDataService" />
      <endpoint address="http://localhost:23183/" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_ISessionService" contract="LimeWs.ISessionService"
        name="BasicHttpBinding_ISessionService" />
    </client>
  </system.serviceModel>
</configuration>